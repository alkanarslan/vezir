{
  "name": "vezir-gateway",
  "version": "1.0.0",
  "description": "A boilerplate to create JavaScript binary executable files using Node.",
  "bin": "server.js",
  "pkg": {
    "assets": [
      "node_modules/**/*",
      "src/**/*"
    ],
    "targets": [
      "node12-macos-x64",
      "node10-win-x64"
    ]
  },
  "dependencies": {
    "express": "^4.17.2",
    "pkg": "^5.5.2",
    "puppeteer": "^13.1.3",
    "puppeteer-core": "^13.1.3"
  }
}
