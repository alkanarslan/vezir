      height: 500,
      top: WINDOW_TOP_OFFSET,
      left: 180,
      destroyOnClose: true,
      recenterAuto: false,
    });
    win.showCenter();
  }
  return WINDOW_ID + "_content";
}

function createWindowForPackageContent() {
  return bynList(WINDOW_ID) + "_content";
}
function commonWindowProcess() {
  var win = Windows.getWindow(WINDOW_ID);
  if (win && win.visible) {
    //pencere daha once acildiysa
    win.setTitle(WINDOW_TITLE);
    win.toFront();
  } else {
    win = new Window({
      id: WINDOW_ID,
      className: class_name,
      title: WINDOW_TITLE,
      width: 825,
      height: 370,
      top: WINDOW_TOP_OFFSET,
      left: 180,
      destroyOnClose: true,
      recenterAuto: false,
    });
    win.showCenter();
  }
  return WINDOW_ID + "_content";
}
//istekGonder, once istegin gonderilip sonra pencere islemlerinin yapildigi durumlarda kullanilir.
function istekGonder(
  url,
  pleaseWaitDiv,
  parameters,
  windowTitle,
  windowId,
  windowProcess
) {
  if (document.getElementById("sorgulaButon"))
    document.getElementById("sorgulaButon").disabled = true; //ilk istek tamamlanmadan ikincisi gonderilmesin diye sorgula dugmesini etkisiz yap
  WINDOW_TITLE = windowTitle; //pencere basligini daha sonra kullanilmak uzere genel bir degiskene sakla
  WINDOW_ID = windowId;
  loadAJAXUsingPOST3(
    url,
    pleaseWaitDiv,
    parameters,
    handleAJAXPostResponseWithShowErrorAndClear,
    windowProcess
  );
}
/*paketIcerigiGoster'in davranisi biraz daha ozel.
 * Ayni id'ye sahip bir pencere daha once acilmissa sunucuya yeni istek gondermiyor.
 * Acilmamissa once istek gonderiyor, ardindan da pencere islemlerini yapiyor. */
function paketIcerigiGoster(poid, pad, pyukz, arsiv) {
  var windowId = "paketIcerik" + poid;

  Windows.closeAllModalWindows();

  var win = Windows.getWindow(windowId);
  if (win && win.visible) {
    //pencere daha once acildiysa
    win.toFront();
  } else {
    var url = "dispatch";
    var parameters =
      (arsiv ? "cmd=ARSIVPAKETICERIK" : "cmd=PAKETICERIK") +
      "&paketOid=" +
      poid;
    var windowTitle =
      labels["PAKET_ICERIGI"] +
      ": <span style=font-weight:normal>" +
      pad +
      " , " +
      pyukz +
      "</span>";
    istekGonder(
      url,
      "pleaseWaitDivForCommon",
      parameters,
      windowTitle,
      windowId,
      createWindowForPackageContent
    );
  }
}
//createAnIframe, belirtilen contentDiv icine bir iframe olusturur.
function createAnIframe(contentDiv, srcUrl, iframeID, width, height) {
  document.getElementById(contentDiv).innerHTML =
    '<iframe src="' +
    srcUrl +
    '" frameborder="0" style="overflow:visible" id="' +
    iframeID +
    '" name="' +
    iframeID +
    '" width="' +
    width +
    '" height="' +
    height +
    '"></iframe>';
}
//emptyIframeURL, paket yukleme isleminden sonra iframe'i yoketmeden once src'yi bos yapar
function emptyIframeURL(iframeID) {
  //sonucu innerHTML ile gosterdigimizden ie'de sayfa yuklemeye devam etmemesi icin src'yi bos yapiyoruz
  try {
    window.parent.document.getElementById(iframeID).src = null;
  } catch (e) {
    document.getElementById(iframeID).src = null;
  }
}

function digerSayfayaGecis(
  frm,
  page,
  pageNo,
  lastPage,
  pagingFormParams,
  contentIdForPaging
) {
  var lst = selectedByn(frm);
  if (lst.length == 0) {
    baskaSayfaGetir(
      page,
      pageNo,
      lastPage,
      pagingFormParams,
      contentIdForPaging
    );
  } else {
    if (confirm(messages["BEYANNAMELISTESI_DIGERSAYFA"])) {
      baskaSayfaGetir(
        page,
        pageNo,
        lastPage,
        pagingFormParams,
        contentIdForPaging
      );
    }
  }
}
function baskaSayfaGetir(
  page,
  pageNo,
  lastPage,
  pagingFormParams,
  contentIdForPaging
) {
  if (!contentIdForPaging)
    contentIdForPaging = Windows.getFocusedWindow().getId() + "_content";

  if (page == "firstPage") {
    pagingFormParams += "pageNo=1";
  } else if (page == "nextPage") {
    pagingFormParams += "pageNo=" + (lastPage > pageNo ? pageNo + 1 : pageNo);
  } else if (page == "previousPage") {
    pagingFormParams += "pageNo=" + (pageNo > 1 ? pageNo - 1 : 1);
  } else {
    pagingFormParams += "pageNo=" + lastPage;
  }
  loadAJAXUsingPOST("dispatch", contentIdForPaging, pagingFormParams);
}
var timeOutHandler = 0;
var activeLayerID = null;
function showMenuLayer(layerID, obj) {
  cancelHidingLayer();
  obj.style.textDecoration = "underline";
  var left = findPosX(obj);
  var top = findPosY(obj) + 20;
  if (activeLayerID != null) hideLayer(activeLayerID);
  var layerObj = document.getElementById(layerID);
  layerObj.style.zIndex = Windows.maxZIndex + 1;
  layerObj.style.left = left;
  layerObj.style.top = top;
  Windows.maxZIndex += 1;
  layerObj.style.visibility = "visible";
  activeLayerID = layerID;
}
function hideLayer(layerID) {
  document.getElementById(layerID).style.visibility = "hidden";
}
function startHidingLayer(layerID, obj) {
  if (obj) obj.style.textDecoration = "";
  timeOutHandler = setTimeout("hideLayer('" + layerID + "')", 500);
}
function cancelHidingLayer() {
  if (timeOutHandler != 0) clearTimeout(timeOutHandler);
}
function slcAltBG(tdObj) {
  tdObj.style.cursor = "pointer";
}
function deslcAltBG(tdObj) {
  tdObj.style.cursor = "default";
}

function callMenuUrlPopUp(url, windowID) {
  var features =
    "toolbar=0, location=0, directories=0, status=0, menubar=0, resizable=1, scrollbars=1, width=500, height=500";
  winRef = window.open(url, windowID, features);
  return false;
}
var paketGonderIFNr = 0;
function createPackageUploadForm() {
  WINDOW_TITLE = labels["MENU_PAKETGONDERME"];
  WINDOW_ID = "paketWindow";
  var win = Windows.getWindow(WINDOW_ID);
  var divName = "";
  if (win && win.visible) {
    win.setTitle(WINDOW_TITLE);
    win.toFront();
    var divNamePrefix = win.getId();
    divName = divNamePrefix + "_content";
  } else {
    divName = createModal();
  }
  paketGonderIFNr++; // her iframe in id si farkli olursa, iframe in history si de farkli olur, back forward dugmeleri ile geri gelemez
  var content =
    '<div><iframe id="pgif' +
    paketGonderIFNr +
    '" frameBorder="0" style="width:99%; height:200px;" src="dispatch?cmd=PAKETYUKLEME&TOKEN=' +
    getTOKEN() +
    '"></iframe>' +
    '<br/><div id="packageControlAndResult"></div>' +
    '<br/><div id="baskaPaketGonder" style="display:none" align="center" ><a href="javascript:createPackageUploadForm()">' +
    labels["YUKLE_BASKA_PAKET"] +
    "</a></div>" +
    "</div>";
  document.getElementById(divName).innerHTML = content;
}

