function gorusOneriFormPost() {
  if (gorusOneriFormControl("gorusOneriForm")) {
    loadAJAXUsingPOST(
      "dispatch",
      "mainWindow_content",
      getParameterStringFromFormID("gorusOneriForm")
    );
    //loadAJAXUsingPOST('dispatch', 'bynGoruntu_content', getParameterStringFromFormID('gorusOneriForm'))
    //istekGonder('dispatch', 'pleaseWaitDivForCommon', getParameterStringFromFormID('gorusOneriForm'), labels['BEYANNAMELISTESI_BEYANNAMELISTESI'], '', createWindowForBynList);
  }
  return false;
}

function gorusOneriFormControl(formID) {
  var form = document.getElementById(formID);
  var eposta = form.eposta;
  if (eposta.value == "") {
    alert(messages["GORUSONERI_EPOSTA_BOS"]);
    eposta.focus();
    return false;
  } else if (eposta.value.search("@") == -1) {
    alert(messages["GORUSONERI_EPOSTA_GECERSIZ"]);
    eposta.focus();
    return false;
  } else if (eposta.value.length < 10) {
    alert(messages["GORUSONERI_EPOSTA_GECERSIZ"]);
    eposta.focus();
    return false;
  }

  var konu = form.konu;
  if (konu.value == "") {
    alert(messages["GORUSONERI_KONU_BOS"]);
    konu.focus();
    return false;
  }

  var gorusOneri = form.gorusOneri;
  if (gorusOneri.value == "") {
    alert(messages["GORUSONERI_BOS"]);
    gorusOneri.focus();
    return false;
  }

  return true;
}
// uses lastLoadedDivName in ajax.js and finds parent window of that div, if any
function getLastLoadedWindowID() {
  var lastDiv = document.getElementById(lastLoadedDivName);
  var parnt = lastDiv == null ? null : lastDiv.parentNode;

  while (parnt != null) {
    if (parnt.nodeName == "DIV" && parnt.className == "dialog")
      // pwc window classname
      return parnt.id;
    parnt = parnt.parentNode;
  }
  return null;
}
function subeBilgiSorgulaFormKontrol(formID) {
  var form = document.getElementById(formID);
  if (form.vergiNo.value.trim() == "") {
    alert("Vergi Kimlik Numarasi bos birakilamaz.");
    form.vergiNo.focus();
    return false;
  }
  if (form.vergiNo.value.trim().length != 10) {
    alert("Vergi Kimlik Numarasi 10 haneli olmalidir.");
    form.vergiNo.focus();
    return false;
  }
  if (!isNumeric(form.vergiNo.value.trim())) {
    alert("Vergi Kimlik Numarasi sadece rakam icermelidir.");
    form.vergiNo.focus();
    return false;
  }
  return true;
}
function subeBilgiSorgulaPost() {
  if (subeBilgiSorgulaFormKontrol("subeBilgiSorgulaForm")) {
    istekGonder(
      "dispatch",
      "pleaseWaitDivForCommon",
      getSubeBilgiSorgulaFormParameters("subeBilgiSorgulaForm"),
      labels["SUBE_BILGISI_SORGULAMA_WNDSONUCBASLIK"],
      "mainWindow",
      commonWindowProcess
    );
  }
  return false;
}
function getSubeBilgiSorgulaFormParameters(formID) {
  var paramsString = "";
  var form = document.getElementById(formID);
  paramsString = "cmd=" + form.cmd.value + "&pageNo=" + form.pageNo.value;
  paramsString +=
    "&vergiNo=" + form.vergiNo.value + "&sorguVD=" + form.sorguVD.value;

  return paramsString;
}
function subeBilgisiSorgulaForm() {
  if (subeBilgiSorgulaFormHTMLKodu != null) {
    var win = showWindowCreateIfNotExist(
      "mainWindow",
      labels["SUBE_BILGISI_SORGULAMA_WNDBASLIK"]
    );
    var cntntDiv = document.getElementById(win.getId() + "_content");
    cntntDiv.innerHTML = subeBilgiSorgulaFormHTMLKodu;
  } else {
    istekGonder(
      "dispatch",
      "pleaseWaitDivForCommon",
      "cmd=SUBEBILGISORGULAEKRAN",
      labels["SUBE_BILGISI_SORGULAMA_WNDBASLIK"],
      "mainWindow",
      commonWindowProcess
    );
  }
}
var subeBilgiSorgulaFormHTMLKodu = null;
