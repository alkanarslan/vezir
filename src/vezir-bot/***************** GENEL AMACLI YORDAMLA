  }
  return false;
}

function maonayHataliSonucuListeGuncelle(bynOid, durumImg, durumTxt) {
  var tdCollection = document.getElementsByTagName("td");
  for (var i = 0; i < tdCollection.length; i++) {
    if (tdCollection[i].id == "checkboxTD" + bynOid) {
      tdCollection[i].innerHTML = "";
    } else if (tdCollection[i].id == "durumTD" + bynOid) {
      tdCollection[i].innerHTML =
        '<img src="' + durumImg + '">&nbsp;' + durumTxt;
    } else if (tdCollection[i].id == "msgIcon" + bynOid) {
      tdCollection[i].innerHTML =
        '<img src="images/ico_msg.gif" title="' +
        labels["BEYANNAMELISTESI_MESAJ"] +
        '" style="cursor:pointer"' +
        "onclick=\"beyannameMesajGosterInDetay('" +
        bynOid +
        "')\">";
    } else if (tdCollection[i].id == "bynPDF" + bynOid) {
      tdCollection[i].innerHTML = "";
    }
  }
}

function maonayHataliSonucuDetayGuncelle(bynOid, durumImg, durumTxt) {
  if (document.getElementById("onayTD"))
    document.getElementById("onayTD").style.visibility = "hidden";
  if (document.getElementById("durumTD"))
    document.getElementById("durumTD").innerHTML =
      '<img src="' + durumImg + '">&nbsp;' + durumTxt;

  if (document.getElementById("messageTD")) {
    document.getElementById("messageTD").innerHTML =
      '<img src="images/ico_msg.gif" style="cursor:pointer" ' +
      'title="' +
      labels["BEYANNAMELISTESI_MESAJ"] +
      '" ' +
      'alt="' +
      labels["BEYANNAMELISTESI_MESAJ"] +
      '" ' +
      "onclick=\"loadAJAXUsingPOST('dispatch', 'bynGoruntu_detay_alani', 'cmd=BEYANNAMEMESAJLISTESI&beyannameOid=" +
      bynOid +
      "')\">";
  }

  if (document.getElementById("beyannameTD")) {
    document.getElementById("beyannameTD").innerHTML = "";
  }
}

function onayIptalSonucuListeGuncelle(
  bynOid,
  durum,
  durumImg,
  durumTxt,
  thkVar,
  tahakkukOid,
  ihbVar
) {
  var tdCollection = document.getElementsByTagName("td");
  for (var i = 0; i < tdCollection.length; i++) {
    if (tdCollection[i].id == "checkboxTD" + bynOid) {
      tdCollection[i].innerHTML = "";
    }
    if (tdCollection[i].id == "onayTD" + bynOid) {
      tdCollection[i].innerHTML = "";
    } else if (tdCollection[i].id == "durumTD" + bynOid) {
      tdCollection[i].innerHTML =
        '<img src="' + durumImg + '">&nbsp;' + durumTxt;
    } else if (tdCollection[i].id == "thkPDF" + bynOid) {
      if (thkVar) {
        //tahakkuk kesilmis mi
        tdCollection[i].innerHTML =
          '<img src="images/pdf_t.gif" title="' +
          labels["PDF_TAHAKKUK_GORUNTULE"] +
          '" alt="' +
          labels["PDF_TAHAKKUK_GORUNTULE"] +
          '" style="cursor:pointer"' +
          "onclick=\"tahakkukGoruntule('" +
          bynOid +
          "','" +
          tahakkukOid +
          "',false)\">";
      }
    } else if (tdCollection[i].id == "ihb" + bynOid) {
      if (ihbVar) {
        tdCollection[i].innerHTML =
          '<span style="cursor:pointer;font-size:12;font-weight:bold" title="' +
          labels["BEYANNAMELISTESI_IHBTOOLTIP"] +
          '"' +
          "onclick=\"ihbarnameleriGetirInDetay('" +
          bynOid +
          "',false)\">" +
          labels["BEYANNAMELISTESI_IHB"] +
          "</span>";
      }
    }
  }
}
// todo taxReturnDisplayde yaptigimiz degisiklikleri burada da yapacagiz
function onayIptalSonucuDetayGuncelle(
  bynOid,
  durumImg,
  durumTxt,
  thkVar,
  thkOid,
  ihbVar
) {
  if (document.getElementById("onayTD"))
    document.getElementById("onayTD").style.visibility = "hidden";
  if (document.getElementById("durumTD"))
    document.getElementById("durumTD").innerHTML =
      '<img src="' + durumImg + '">&nbsp;' + durumTxt;

  if (thkVar) {
    //tahakkuk kesilmis mi
    if (document.getElementById("tahakkukTD")) {
      document.getElementById("tahakkukTD").innerHTML =
        '<img src="images/pdf_t.gif" style="cursor:pointer" ' +
        'title="' +
        labels["PDF_TAHAKKUK_GORUNTULE"] +
        '"' +
        'alt="' +
        labels["PDF_TAHAKKUK_GORUNTULE"] +
        '"' +
        "onclick=\"tahakkukGoruntule('" +
        bynOid +
        "','" +
        thkOid +
        "',false)\">";
    }
    if (ihbVar) {
      if (document.getElementById("ihbarnameTD")) {
        document.getElementById("ihbarnameTD").innerHTML =
          '<span style="cursor:pointer;font-size:12;font-weight:bold"' +
          'title="' +
          labels["BEYANNAMELISTESI_IHBTOOLTIP"] +
          '"' +
          "onclick=\"loadAJAXUsingPOST('dispatch', 'bynGoruntu_detay_alani', 'cmd=IHBARNAMELERIGETIR&beyannameOid=" +
          bynOid +
          "')\">" +
          labels["BEYANNAMELISTESI_IHB"] +
          "</span>";
      }
    }
  }
}
function checkNewParola() {
  var sifre = document.getElementById("sifre");
  if (sifre.value == "") {
    alert(messages["LUTFEN_SIFRENIZI_GIRINIZ"]);
    sifre.focus();
    return false;
  }

  var parola = document.getElementById("parola");

  if (parola.value == "") {
    alert(messages["LUTFEN_PAROLANIZI_GIRINIZ"]);
    parola.focus();
    return false;
  }

  var x = document.getElementById("yeniParola").value;

  if (x.length > 8 || x.length < 6) {
    alert(messages["DEGISIKLIK_PAROLA_UZUNLUK8"]);
    document.getElementById("yeniParola").focus();
    return false;
  }

  if (x.search(labels["TURKCE_KARAKTERLER"]) != -1 || x.search(" ") != -1) {
    alert(messages["DEGISIKLIK_PAROLA_TURKCE"]);
    document.getElementById("yeniParola").focus();
    return false;
  }

  if (x != document.getElementById("yeniParolaTekrar").value) {
    alert(messages["DEGISIKLIK_PAROLA_AYNI_DEGIL"]);
    document.getElementById("yeniParolaTekrar").focus();
    return false;
  }
  return true;
}
function postNewParolaForm() {
  if (checkNewParola()) {
    loadAJAXUsingPOST(
      "dispatch",
      "mainWindow_content",
      getParameterStringFromFormID("formParola")
    );
  }
  return false;
}
function checkNewSifre() {
  var sifre = document.getElementById("sifre");

  if (sifre.value == "") {
    alert(messages["LUTFEN_SIFRENIZI_GIRINIZ"]);
    sifre.focus();
    return false;
  }

  var parola = document.getElementById("parola");

  if (parola.value == "") {
    alert(messages["LUTFEN_PAROLANIZI_GIRINIZ"]);
    parola.focus();
    return false;
  }

  var re6digit = /^\d{6}$/;
  if (
    document.getElementById("yeniSifre").value.search(re6digit) == -1 ||
    document.getElementById("yeniSifre").value.length != 6
  ) {
    alert(messages["DEGISIKLIK_SIFRE_SAYISAL6"]);
    document.getElementById("yeniSifre").focus();
    return false;
  }

  if (
    document.getElementById("yeniSifre").value !=
    document.getElementById("yeniSifreTekrar").value
  ) {
    alert(messages["DEGISIKLIK_SIFRE_AYNI_DEGIL"]);
    document.getElementById("yeniSifreTekrar").focus();
    return false;
  }
  return true;
}
function postNewSifreForm() {
  if (checkNewSifre()) {
    loadAJAXUsingPOST(
      "dispatch",
      "mainWindow_content",
      getParameterStringFromFormID("formSifre")
    );
  }
  return true;
}
var userMobilTel = "";
var userFaks = "";
var userEPosta = "";
function checkNewInfo() {
  var re10digit = /^\d{10}$/;
  var mobilTel = document.getElementById("mobilTel");
  if (
    mobilTel.value != "" &&
    (mobilTel.value.search(re10digit) == -1 || mobilTel.value.length != 10)
  ) {
    alert(messages["DEGISIKLIK_INFO_MOBILTEL"]);
    mobilTel.focus();
    return false;
  }
  var faks = document.getElementById("faks");
  if (
    faks.value != "" &&
    (faks.value.search(re10digit) == -1 || faks.value.length != 10)
  ) {
    alert(messages["DEGISIKLIK_INFO_FAKSNO"]);
    faks.focus();
    return false;
  }
  ePostaRegEx = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
  var x = document.getElementById("ePosta");
  if (x.value != "" && x.value.search(ePostaRegEx) == -1) {
    alert(messages["DEGISIKLIK_INFO_EPOSTA"]);
    x.focus();
    return false;
  }
  if (
    mobilTel.value == userMobilTel &&
    faks.value == userFaks &&
    x.value == userEPosta
  ) {
    alert(messages["BILGILERINIZDE_DEGISIKLIK_YAPMADINIZ"]);
    mobilTel.focus();
    return false;
  }
  return true;
}
function postNewInfo() {
  if (checkNewInfo()) {
    loadAJAXUsingPOST(
      "dispatch",
      "mainWindow_content",
      getParameterStringFromFormID("formInfo")
    );
  }
  return false;
}
function postTaxReturnSearchArchiveForm() {
  if (taxReturnSearchFormControl("taxReturnSearchFormArchive")) {
    //normal taxReturnSearch ile ayni oldugu icin farkli metodlar kullanmaya gerek yoktur
    istekGonder(
      "dispatch",
      "pleaseWaitDivForCommon",
      getTaxReturnSearchFormParameters("taxReturnSearchFormArchive"),
      labels["BEYANNAMELISTESI_ARSIV"],
      "",
      createWindowForBynList
    );
  }
  return true;
}
function enableControl(obj, enabled) {
  obj.style.enabled = enabled;
}

function isCbSelected(cb) {
  return eval(cb + ".checked");
}
function stkVeKGeciciDisindaBeyannameBirlikteSecilmis() {
  //STK secildiyse beyanname tipi sadece KGECICI olabilir
  if (!kGeciciSecilmis() && stkSecilmis()) return true;
  return false;
}
function kGeciciSecilmis() {
  if (beyannameTipi == "KGECICI") return true;
  return false;
}
function stkSecilmis() {
  return isCbSelected("document.ozelOnayForm.STK");
}
function subeSecilebilirmi() {
  var subecmb = eval("document.ozelOnayForm.SUBENO");
  return subecmb == undefined ? false : true;
}
function ozelIsaretOnay() {
  return confirm(
    "Beyanname için herhangi bir özel işaret seçmediniz devam etmek istiyor musunuz?"
  );
}
function herBeyannameIcinEnAzBirOzelIsaretSecilmis() {
  var j;

  var secili = false;
  for (j = 0; j < ozelIsaretler.length; j++) {
    if (isCbSelected("document.ozelOnayForm." + ozelIsaretler[j])) {
      secili = true;
      break;
    }
  }
  if (!secili) return false;
  return true;
}
function kkdfPismanlikSecilmis() {
  //duzeltmeyle pismanlik beraber secilebilir ama sadece pismalik secilemez

  if (beyannameTipi == "KKDF" && isCbSelected("document.ozelOnayForm.PIS"))
    return true;
  return false;
}
function pismanlikKSSBirlikteSecilmis() {
  if (
    isCbSelected("document.ozelOnayForm.PIS") &&
    isCbSelected("document.ozelOnayForm.KSS")
  )
    return true;
  return false;
}
function pismanlikIhtiraziIleBirlikteSecilmis() {
  if (
    isCbSelected("document.ozelOnayForm.PIS") &&
    isCbSelected("document.ozelOnayForm.IHT")
  )
    return true;
  return false;
}
function enAzBirPismanlikVeyaKSSVeyaDUZSecilmis() {
  if (
    isCbSelected("document.ozelOnayForm.PIS") ||
    isCbSelected("document.ozelOnayForm.KSS") ||
    isCbSelected("document.ozelOnayForm.DZT") ||
    isCbSelected("document.ozelOnayForm.IHT") ||
    isCbSelected("document.ozelOnayForm.DVM") ||
    izahSecilmis() ||
    _7326Secilmis()
  )
    return true;
  return false;
}
function enAzBirDamgaVergisiMuafiyetiSecilmis() {
  if (isCbSelected("document.ozelOnayForm.DVM")) return true;
  return false;
}

function kssSecilmis() {
  if (isCbSelected("document.ozelOnayForm.KSS")) return true;
  return false;
}

function kssVeDuzeltmeBirlikteSecilmis() {
  if (
    isCbSelected("document.ozelOnayForm.KSS") &&
    isCbSelected("document.ozelOnayForm.DZT")
  )
    return true;
  return false;
}

function pismanlikSecilmis() {
  if (isCbSelected("document.ozelOnayForm.PIS")) return true;
  return false;
}
function ihtiraziSecilmis() {
  if (isCbSelected("document.ozelOnayForm.IHT")) return true;
  return false;
}
function vergiAffiSecilmis() {
  if (!vaMumkun())
    // 6111 daha hizmete acilmadiysa veya kapandiysa sayfada olmayabilir
    return false;
  if (isCbSelected("document.ozelOnayForm._6111")) return true;
  return false;
}
function vergiAffi2021Secilmis() {
  if (!va2021Mumkun())
    // 7326 daha hizmete acilmadiysa veya kapandiysa sayfada olmayabilir
    return false;
  if (isCbSelected("document.ozelOnayForm._7326")) return true;
  return false;
}
function duzeltmeSecilmis() {
  if (isCbSelected("document.ozelOnayForm.DZT")) return true;
  return false;
}
function dvmSecilmis() {
  var dvm = eval("document.ozelOnayForm.DVM") == undefined ? false : true;
  if (dvm) if (isCbSelected("document.ozelOnayForm.DVM")) return true;
  return false;
}
function _6736Secilmis() {
  var dvm = eval("document.ozelOnayForm._6736") == undefined ? false : true;
  if (dvm) if (isCbSelected("document.ozelOnayForm._6736")) return true;
  return false;
}
function _7143Secilmis() {
  var dvm = eval("document.ozelOnayForm._7143") == undefined ? false : true;
  if (dvm) if (isCbSelected("document.ozelOnayForm._7143")) return true;
  return false;
}
function _7326Secilmis() {
  var dvm = eval("document.ozelOnayForm._7326") == undefined ? false : true;
  if (dvm) if (isCbSelected("document.ozelOnayForm._7326")) return true;
  return false;
}
function vefatSecilmis() {
  var dvm = eval("document.ozelOnayForm.VFT") == undefined ? false : true;
  if (dvm) if (isCbSelected("document.ozelOnayForm.VFT")) return true;
  return false;
}
function izahSecilmis() {
  var izah = eval("document.ozelOnayForm.IZAH") == undefined ? false : true;
  if (izah) if (isCbSelected("document.ozelOnayForm.IZAH")) return true;
  return false;
}
function vaMumkun() {
  return arrIndexOf(ozelIsaretler, "_6111") != -1;
}
function va2021Mumkun() {
  return arrIndexOf(ozelIsaretler, "_7326") != -1;
}
function arrIndexOf(arr, item) {
  var length = arr.length;
  for (var i = 0; i < length; i++) if (arr[i] === item) return i;
  return -1;
}

var tahakkukKesilenBeyanname = false;

function ozelOnayla() {
  /*if(stkVeKGeciciDisindaBeyannameBirlikteSecilmis())
    {
        alert(messages["BEYANNAMEOZELONAY_VERILENBEYANNAMEILE_STKSECILEMEZ"]);
        return false;
    }*/
  if (subeSecilebilirmi() && !ozelIsaretOnay()) {
    return false;
  }
  if (!subeSecilebilirmi() && !herBeyannameIcinEnAzBirOzelIsaretSecilmis()) {
    alert(messages["BEYANNAMEOZELONAY_ENAZBIROZELISARETSECILMELI"]);
    return false;
  }
  if (pismanlikKSSBirlikteSecilmis()) {
    alert(messages["BEYANNAMEOZELONAY_PISMANLIKVEKSSBIRLIKTESECILEMEZ"]);
    return false;
  }
  if (pismanlikIhtiraziIleBirlikteSecilmis()) {
    alert(messages["BEYANNAMEONAY_PISSECILDIYSEIHTSECILEMEZ"]);
    return false;
  }
  if (vergiAffiSecilmis() && enAzBirDamgaVergisiMuafiyetiSecilmis()) {
    alert(messages["BEYANNAMEONAY_6111SECILDIYSEDVMSECILEMEZ"]);
    return false;
  }
  if (vergiAffiSecilmis() && ihtiraziSecilmis()) {
    alert(messages["BEYANNAMEONAY_6111SECILDIYSEIHTSECILEMEZ"]);
    return false;
  }
  if (
    vergiAffiSecilmis() &&
    !duzeltmeSecilmis() &&
    !(kssSecilmis() || pismanlikSecilmis())
  ) {
    alert(messages["BEYANNAMEONAY_6111SECILDIYSE_KSSPISSECILMELI"]);
    return false;
  }
  if (
    vergiAffiSecilmis() &&
    duzeltmeSecilmis() &&
    !(kssSecilmis() || pismanlikSecilmis())
  ) {
    alert(messages["BEYANNAMEONAY_6111DZTSECILDIYSE_KSSPISSECILMELI"]);
    return false;
  }
  /*if (vergiAffi2021Secilmis() && enAzBirDamgaVergisiMuafiyetiSecilmis())
    {
        alert(messages["BEYANNAMEONAY_7326SECILDIYSEDVMSECILEMEZ"]);
        return false;
    }*/
  if (vergiAffi2021Secilmis() && ihtiraziSecilmis()) {
    alert(messages["BEYANNAMEONAY_7326SECILDIYSEIHTSECILEMEZ"]);
    return false;
  }
  if (
    vergiAffi2021Secilmis() &&
    !duzeltmeSecilmis() &&
    !(kssSecilmis() || pismanlikSecilmis())
  ) {
    alert(messages["BEYANNAMEONAY_7326SECILDIYSE_KSSPISSECILMELI"]);
    return false;
  }
  if (
    vergiAffi2021Secilmis() &&
    duzeltmeSecilmis() &&
    !(kssSecilmis() || pismanlikSecilmis())
  ) {
    alert(messages["BEYANNAMEONAY_7326DZTSECILDIYSE_KSSPISSECILMELI"]);
    return false;
  }
  /*if(kGeciciSecilmis())
    {
        if(stkSecilmis())
        {
            if (!confirm(messages["KGECICI6552KAPSAMINDA_BEY_VER"]))
                return false;
        }
        else
        {
            if (!confirm(messages["KGECICI6552KAPSAMINDA_BEY_VERME"]))
                return false;
        }
    }*/
  if (pismanlikSecilmis()) {
    if (!confirm(messages["BEYANNAMEOZELONAY_PISMANLIK371KAPSAMINDAOLMALI"]))
      return false;
  }
  if (enAzBirDamgaVergisiMuafiyetiSecilmis()) {
    if (
      !confirm(messages["BEYANNAMEOZELONAY_DAMGAVERGISIMUAFIYETIEMINMISINIZ"])
    )
      return false;
  }
  if (kkdfPismanlikSecilmis()) {
    alert(messages["BEYANNAMEOZELONAY_KKDFPISOLAMAZ"]);
    return false;
  }
  if (izahSecilmis() && (kssSecilmis() || pismanlikSecilmis())) {
    alert(messages["BEYANNAMEOZELONAY_IZAH_PISMANLIKVEKSSBIRLIKTESECILEMEZ"]);
    return false;
  }
  if (izahSecilmis() && ihtiraziSecilmis()) {
    alert(messages["BEYANNAMEOZELONAY_IZAH_IHTBIRLIKTESECILEMEZ"]);
    return false;
  }
  if (izahSecilmis() && dvmSecilmis()) {
    alert(messages["BEYANNAMEOZELONAY_IZAH_DVMBIRLIKTESECILEMEZ"]);
    return false;
  }
  if (izahSecilmis() && _6736Secilmis()) {
    alert(messages["BEYANNAMEOZELONAY_IZAH_6736BIRLIKTESECILEMEZ"]);
    return false;
  }
  if (izahSecilmis() && _7143Secilmis()) {
    alert(messages["BEYANNAMEOZELONAY_IZAH_7143BIRLIKTESECILEMEZ"]);
    return false;
  }
  if (izahSecilmis() && _7326Secilmis()) {
    alert(messages["BEYANNAMEOZELONAY_IZAH_7326BIRLIKTESECILEMEZ"]);
    return false;
  }
  if (izahSecilmis() && vefatSecilmis()) {
    alert(messages["BEYANNAMEOZELONAY_IZAH_VFTBIRLIKTESECILEMEZ"]);
    return false;
  }
  if (
    kssSecilmis() &&
    vaMumkun() &&
    !vergiAffiSecilmis() &&
    tahakkukKesilenBeyanname
  ) {
    if (
      !confirm(
        messages[
          "BEYANNAMEOZELONAY_KSS_SECILDI_VA_SECILMEDI_ONAYLAMAK_ISTIYOR_MUSUN"
        ]
      )
    )
      return false;
  }
  if (
    pismanlikSecilmis() &&
    vaMumkun() &&
    !vergiAffiSecilmis() &&
    tahakkukKesilenBeyanname
  ) {
    if (
      !confirm(
        messages[
          "BEYANNAMEOZELONAY_PIS_SECILDI_VA_SECILMEDI_ONAYLAMAK_ISTIYOR_MUSUN"
        ]
      )
    )
      return false;
  } else {
    if (!confirm(messages["BEYANNAMELISTESI_OZELONAYLAMAK_ISTIYOR_MUSUN"]))
      return false;
  }

  if (tahakkukKesilenBeyanname) {
    if (
      kssSecilmis() &&
      !duzeltmeSecilmis() &&
      !kkdfPismanlikSecilmis() &&
      !(beyannameTipi == "POSET")
    )
      if (!confirm(messages["BEYANNAMEOZELONAY_KSSSECILMIS"])) return false;
    if (
      kssVeDuzeltmeBirlikteSecilmis() &&
      !kkdfPismanlikSecilmis() &&
      !(beyannameTipi == "POSET")
    )
      if (!confirm(messages["BEYANNAMEOZELONAY_KSS_DZT_BIRLIKTE_SECILMIS"]))
        return false;
  }
  if (enAzBirPismanlikVeyaKSSVeyaDUZSecilmis())
    document.ozelOnayForm.cmd.value = "BEYANNAMEPISMANLIKONAY";
  return true;
}

function pismanlikTrim(s) {
  return s.replace(/^\s+|\s+$/, "");
}

function isCMBDefined() {
  var cmb = eval("document.poBeyannameList.mg");
  return cmb == undefined ? false : true;
}

function isDVMSecebilir(dvmVar, dvmMuafiyetDurumu) {
  if (dvmVar) {
    if (dvmMuafiyetDurumu != null && dvmMuafiyetDurumu == "3") return false;
  }
  return true;
}

function isCmbSelected() {
  var cmb = eval("document.poBeyannameList.mg");
  if (cmb == undefined) return true;
  return cmb.selectedIndex > 0;
}

function isMukerrer355CmbSelected() {
  var cb = eval("document.poBeyannameList.i355");
  if (cb == undefined || cb.checked) return true;
  return false;
}

function isTextAreaEmpty(t) {
  //var t = eval("document.poBeyannameList.grk");
  if (t == undefined) return false;
  return pismanlikTrim(t.value) == "";
}
function tumMukellefGruplariSecilmis() {
  if (!isCmbSelected()) return false;
  return true;
}
var maxlimit = 255;
function isGerekceOutOfLength() {
  var t = eval("document.poBeyannameList.grk");
  if (t) {
    if (t.value.length > maxlimit) return false;
  }
  return true;
}

function isDVMGerekceOutOfLength() {
  var t = eval("document.poBeyannameList.dvmGrk");
  if (t) {
    if (t.value.length > maxlimit) return false;
  }
  return true;
}

function isIzahOnayKoduDolu() {
  var t = eval("document.poBeyannameList.izahOnayKodu");
  if (t) {
    if (t.value.trim() == "") return false;
  }
  return true;
}

/*function isIzahTarihlerDolu()
{
    var t1 = eval("document.poBeyannameList.izahTebligTarih");
    var t2 = eval("document.poBeyannameList.izahIzahTarih");
    if(t1 && t2){
        if(t1.value == "" && t2.value == "")
            return false;
    }
    return true;
}*/

function isGerekceDolu() {
  if (isTextAreaEmpty(eval("document.poBeyannameList.grk"))) return false;
  return true;
}

function isIhtirazGerekceDolu() {
  if (isTextAreaEmpty(eval("document.poBeyannameList.ihtGrk"))) return false;
  return true;
}

function isDVMGerekceDolu() {
  if (isTextAreaEmpty(eval("document.poBeyannameList.dvmGrk"))) return false;
  return true;
}

function odemeSekliDegisti() {
  var odemeSekliDefaten = document.poBeyannameList.odemeSekli[0].checked;

  document.getElementById("taksitSecenekleri").style.display = odemeSekliDefaten
    ? "none"
    : "";
  if (odemeSekliDefaten) {
    document.poBeyannameList.taksitSayisi.value = "1";
    document.poBeyannameList.taksitYazi.value = "";
  } else document.poBeyannameList.taksitSayisi.value = "";
}
function isTaksitSayisiGecerli() {
  var tY = eval("document.poBeyannameList.taksitYazi");
  var tS = eval("document.poBeyannameList.taksitSayisi");

  if (tY && tS) {
    var odemeSekliDefaten = document.poBeyannameList.odemeSekli[0].checked;

    if (odemeSekliDefaten) {
      document.poBeyannameList.taksitSayisi.value = "1";
      return true;
    } else {
      var taksitSecenekleriArray = new Array("6", "9", "12", "18");
      var taksitSecenekleriYaziylaArray = new Array(
        "altı",
        "dokuz",
        "oniki",
        "onsekiz"
      );
      var taksitSecenekleriYaziylaArray2 = new Array(
        "altı",
        "dokuz",
        "on iki",
        "on sekiz"
      );

      var taksitYazi = document.poBeyannameList.taksitYazi.value;
      var taksitSayisi = document.poBeyannameList.taksitSayisi.value;
      var gecerli = false;
      for (var i = 0; i < taksitSecenekleriArray.length; i++) {
        if (taksitSayisi == taksitSecenekleriArray[i]) {
          gecerli = true;
          break;
        }
      }
      if (!gecerli) return false;
      else {
        if (
          !(
            taksitYazi == taksitSecenekleriYaziylaArray[i] ||
            taksitYazi == taksitSecenekleriYaziylaArray2[i]
          )
        )
          return false;
      }
    }
  }
  return true;
}

function pismanlikOnayla(
  pisKssveyaIzah,
  isBirinciSinifMukellef,
  dvmVar,
  dvmMuafiyetDurumu
) {
  if (pisKssveyaIzah) {
    document.poBeyannameList.cmd.value = "BEYANNAMEONAY_THKMIKTARLARI";
    document.poBeyannameList.VTYAZ.value = "0";
  }
  if (!tumMukellefGruplariSecilmis()) {
    alert(messages["BEYANNAMEPISMANLIKONAY_HERBEYANNAMEICINMUKGRUBUSECILMELI"]);
    return false;
  }
  if (!isGerekceDolu()) {
    alert(messages["BEYANNAMEPISMANLIKONAY_DUZELTMEGEREKCESIDOLDURULMALI"]);
    return false;
  }
  if (!isIhtirazGerekceDolu()) {
    alert(messages["BEYANNAMEIHTGEREKCESIDOLDURULMALI"]);
    return false;
  }
  if (!isGerekceOutOfLength()) {
    alert(messages["BEYANNAMEPISMANLIKONAY_DUZELTMEGEREKCESIMAX255"]);
    return false;
  }
  if (!isTaksitSayisiGecerli()) {
    alert(messages["BEYANNAMEONAY_MATRAHARTIRIMI_TAKSITSAYISI_HATALI"]);
    return false;
  }
  if (!isDVMGerekceDolu()) {
    alert(messages["BEYANNAMEPISMANLIKONAY_DVMGEREKCESIDOLDURULMALI"]);
    return false;
  }
  if (!isDVMGerekceOutOfLength()) {
    alert(messages["BEYANNAMEPISMANLIKONAY_DVMGEREKCESIMAX255"]);
    return false;
  }
  if (!isIzahOnayKoduDolu()) {
    alert(messages["BEYANNAMEONAY_IZAHONAYKODU_BOS"]);
    return false;
  }
  /*if(!isIzahTarihlerDolu()){
        alert(messages["BEYANNAMEONAY_IZAHTARIHLER_BOS"]);
        return false;
    }*/
  if (isCMBDefined()) {
    var mkGrup = eval("document.poBeyannameList.mg");
    var mkGrupIndex = mkGrup.selectedIndex;
    if (!(mkGrupIndex == 1 || mkGrupIndex == 2)) {
      if (isBirinciSinifMukellef) {
        if (
          !confirm(messages["BEYANNAMEPISMANLIKONAY_BIRINCISINIFMUKELLEFDEGIL"])
        )
          return false;
      }
    }
  }
  if (!isDVMSecebilir(dvmVar, dvmMuafiyetDurumu)) {
    alert(messages["BEYANNAMEPISMANLIKONAY_DVMISARETLEYEMEZ"]);
    return false;
  }
  if (
    (beyannameTipi == "FORMBA" || beyannameTipi == "FORMBS") &&
    !isMukerrer355CmbSelected()
  ) {
    if (
      !confirm(
        messages[
          "BEYANNAMEPISMANLIKONAY_FORMBABS_CEZASIZ_ONAYLAMAK_ISTIYOR_MUSUN"
        ]
      )
    ) {
      return false;
    }
  }

  if (!confirm(messages["BEYANNAMEPISMANLIKONAY_ONAYLAMAK_ISTIYOR_MUSUN"]))
    return false;

  //    document.poBeyannameList.onayButton.disabled = true;
  //    document.poBeyannameList.submit();
  return true;
}

function pismanlikMiktarlariniOnayla() {
  if (
    confirm(
      messages["BEYANNAMEPISMANLIKONAY_MIKTARLAR_ONAYLAMAK_ISTIYOR_MUSUN"]
    )
  ) {
    var form = document.getElementById("pismanlikOnayThkMik");
    form.cmd.value = "BEYANNAMEONAY";
    loadAJAXUsingPOST(
      "dispatch",
      "bynGoruntu_detay_alani",
      getParameterStringFromFormID("pismanlikOnayThkMik")
    );
  }
  return false;
}

