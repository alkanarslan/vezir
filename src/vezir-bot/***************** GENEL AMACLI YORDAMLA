function getTaxReturnSearchEskiMuhasebeciFormParameters(formID) {
  var paramsString = "";
  var form = document.getElementById(formID);
  paramsString = "cmd=" + form.cmd.value;
  paramsString += "&sorguTipiN=1&vergiNo=" + form.vergiNo.value;
  paramsString += "&sorguTipiB=1&beyannameTanim=" + form.beyannameTanim.value;
  paramsString += "&sorguTipiP=1&donemBasYil=" + form.donemBasYil.value;
  paramsString += "&donemBasAy=" + form.donemBasAy.value;
  paramsString += "&donemBitYil=" + form.donemBitYil.value;
  paramsString += "&donemBitAy=" + form.donemBitAy.value;
  return paramsString;
}
function getRadioCheckedValue(radioObj) {
  if (!radioObj) return "";
  var radioLength = radioObj.length;
  if (radioLength == undefined)
    if (radioObj.checked) return radioObj.value;
    else return "";
  for (var i = 0; i < radioLength; i++) {
    if (radioObj[i].checked) {
      return radioObj[i].value;
    }
  }
  return "";
}
function packageSearchFormKontrol(formID) {
  var form = document.getElementById(formID);

  if (!form.sorguTipiZ.checked) {
    alert("Tarih Aralığı seçilmelidir.");
    form.baslangicTarihiGun.focus();
    return false;
  } else {
    var basTar = form.baslangicTarihi.value;
    var bitTar = form.bitisTarihi.value;
    if (!yuklemeTarihAraligiGecerli(basTar, bitTar)) {
      form.baslangicTarihiGun.focus();
      return false;
    }
  }

  if (!(form.sorguTipiZ.checked || form.sorguTipiD.checked)) {
    alert(messages["PAKETSORGU_ENAZ_BIR_KRITER"]);
    form.sorguTipiZ.focus();
    return false;
  }
  if (form.sorguTipiD.checked && !form.sorguTipiZ.checked) {
    alert(messages["PAKETSORGU_SADECE_DURUM"]);
    form.sorguTipiZ.focus();
    return false;
  }
  return true;
}

function oncekiYilGgeciciTahsilatFormKontrol(formID) {
  var form = document.getElementById(formID);
  if (form.mukellefVergiNo.value == null || form.mukellefVergiNo.value == "") {
    //alert(messages['PAKETSORGU_ENAZ_BIR_KRITER']);
    alert("Vergi Numarasi kriteri bos birakilamaz.");
    form.mukellefVergiNo.focus();
    return false;
  }
  return true;
}

