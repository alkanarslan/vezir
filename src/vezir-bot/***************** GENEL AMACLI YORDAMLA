function izahOnayKoduChanged() {
  document.getElementById("izahTebligTarih").value = "";
  document.getElementById("izahTebligTarihGun").value = "";
  document.getElementById("izahTebligTarihAy").value = "";
  document.getElementById("izahTebligTarihYil").value = "";
  document.getElementById("izahTebligTarihBtn").style.display = "";

  document.getElementById("izahIzahTarih").value = "";
  document.getElementById("izahIzahTarihGun").value = "";
  document.getElementById("izahIzahTarihAy").value = "";
  document.getElementById("izahIzahTarihYil").value = "";
  document.getElementById("izahIzahTarihBtn").style.display = "";
}

function izahTarihlerGetir(beyoid) {
  var onaykodu = document.getElementById("izahOnayKodu").value.trim();

  if (onaykodu == "") {
    alert(messages["BEYANNAMEONAY_IZAHONAYKODU_BOS"]);
    return;
  }

  loadAJAXUsingPOST3(
    "dispatch",
    "pleaseWaitDivForCommon",
    "cmd=BEYANNAMEIZAHONAYTARIHGETIR&onayKodu=" +
      onaykodu +
      "&beyoid=" +
      beyoid,
    handleAJAXPostResponseWithShowErrorAndClear,
    "izahOnayTarihGetirdiv"
  );
}

function islemYap(textAreaName) {
  var form = document.getElementById("poBeyannameList");
  var tex;
  if (textAreaName == "grk") var tex = form.grk.value;
  else if (textAreaName == "ihtGrk") var tex = form.ihtGrk.value;
  else if (textAreaName == "dvmGrk") var tex = form.dvmGrk.value;
  else if (textAreaName == "izahGrk") var tex = form.izahGrk.value;
  else if (textAreaName == "sgkThkGrkAciklama")
    var tex = form.sgkThkGrkAciklama.value;

  var length = tex.length;
  if (length > maxlimit) {
    tex = tex.substring(0, maxlimit);
    if (textAreaName == "grk") form.grk.value = tex;
    else if (textAreaName == "ihtGrk") form.ihtGrk.value = tex;
    else if (textAreaName == "dvmGrk") form.ihtGrk.value = tex;
    else if (textAreaName == "izahGrk") form.izahGrk.value = tex;
    else if (textAreaName == "sgkThkGrkAciklama")
      form.sgkThkGrkAciklama.value = tex;
    return false;
  }

  if (textAreaName == "grk") form.limit.value = maxlimit - length;
  if (textAreaName == "ihtGrk") form.ihtLimit.value = maxlimit - length;
  if (textAreaName == "dvmGrk") form.dvmLimit.value = maxlimit - length;
  if (textAreaName == "izahGrk") form.izahLimit.value = maxlimit - length;
  if (textAreaName == "sgkThkGrkAciklama")
    form.sgkLimit.value = maxlimit - length;
}

/***************** JAVASCRIPT BUSINESS YORDAMLARI *********************/
var class_name = "alphacube"; //acilir pencerelerde kullanilacak
var WINDOW_TITLE = ""; //parametre gecilemeyen fonksiyonlarda kullanilacak
var WINDOW_ID = "";
var WINDOW_TOP_OFFSET = 150;
var BYN_LISTESI_SAYISI = 0;

function createModal() {
  var bMinimizable = true;
  var winHeight = 330;
  if (WINDOW_ID == "bynGoruntu") {
    //beyanname detayi penceresi kucultulemesin.
    winHeight = 420;
    bMinimizable = false;
  } else if (WINDOW_ID == "onayIptalSonuclariWindow") {
    winHeight = 390;
  }
  var win = Windows.getWindow(WINDOW_ID);
  if (!win) {
    //pencere daha once acildiysa
    //win.setTitle(WINDOW_TITLE);
    //win.toFront();

    win = new Window({
      id: WINDOW_ID,
      title: WINDOW_TITLE,
      minimizable: bMinimizable,
      className: class_name,
      top: 140,
      left: 100,
      width: 825,
      height: winHeight,
      destroyOnClose: true,
      recenterAuto: false,
    });

    win.showCenter(true);
    /*if (WINDOW_ID == "bynGoruntu") {
         win.showCenter();//beyanname detayı pencerenin ortasında gosteriyoruz
         }
         else {
         win.show(true);//modal yaratiyor
         }*/
  }
  return WINDOW_ID + "_content";
}
function bynList(wid) {
  var no = BYN_LISTESI_SAYISI;
  BYN_LISTESI_SAYISI++;
  var carpan = no % 3;
  if (!wid) wid = "bynList" + no;
  var win = new Window({
    id: wid,
    className: class_name,
    title: WINDOW_TITLE,
    width: 825,
    height: 710,
    top: WINDOW_TOP_OFFSET + (carpan + 1) * 20,
    left: 20 + (carpan + 1) * 25,
    destroyOnClose: true,
    recenterAuto: false,
  });
  win.show();
  return wid;
}

function createWindowForBynList() {
  Windows.closeAllModalWindows();
  return bynList() + "_content";
}

function createWindowForVerilmemisBynList() {
  var win = Windows.getWindow(WINDOW_ID);
  if (win && win.visible) {
    //pencere daha once acildiysa
    win.setTitle(WINDOW_TITLE);
    win.toFront();
  } else {
    win = new Window({
      id: WINDOW_ID,
      className: class_name,
      title: WINDOW_TITLE,
      width: 850,
      height: 500,
      top: WINDOW_TOP_OFFSET,
      left: 180,
      destroyOnClose: true,
      recenterAuto: false,
    });
    win.showCenter();
  }
  return WINDOW_ID + "_content";
}
function createWindowForGeneralPurpose() {
  var win = Windows.getWindow(WINDOW_ID);
  if (win && win.visible) {
    //pencere daha once acildiysa
    win.setTitle(WINDOW_TITLE);
