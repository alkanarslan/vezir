function getParameterForArsiv(arsiv) {
  var arsivFlag = "";
  if (arsiv) arsivFlag = "&ARSIV=T";

  return arsivFlag;
}
function isNumeric(value) {
  if (!/\D/.test(value)) return true;
  //IF NUMBER
  else return false;
}

function confirmProcess(process, message) {
  if (confirm(message)) {
    process();
  }
  return false;
}
var paginationOidsByWindowID = {};

function digerSayfayaGecisDurumaGore(
  frm,
  page,
  pageNo,
  birSayfadaGosterilecekSay,
  cmd
) {
  var lst = selectedByn(frm);
  if (lst.length == 0) {
    getOids(page, pageNo, birSayfadaGosterilecekSay, cmd);
  } else {
    if (confirm(messages["BEYANNAMELISTESI_DIGERSAYFA"])) {
      getOids(page, pageNo, birSayfadaGosterilecekSay, cmd);
    }
  }
}

function getOids(page, pageNo, birSayfadaGosterilecekSay, cmd, windowID) {
  if (!windowID) windowID = Windows.getFocusedWindow().getId();
  var oidsForWindow = paginationOidsByWindowID[windowID];
  var oidSayisi = oidsForWindow.length;
  var lastPage =
    oidSayisi % birSayfadaGosterilecekSay > 0
      ? Math.floor(oidSayisi / birSayfadaGosterilecekSay) + 1
      : oidSayisi / birSayfadaGosterilecekSay;

  //sayfa numarasini belirle
  if (page == "firstPage") {
    pageNo = 1;
  } else if (page == "nextPage") {
    pageNo = lastPage > pageNo ? pageNo + 1 : pageNo;
  } else if (page == "previousPage") {
    pageNo = pageNo > 1 ? pageNo - 1 : 1;
  } else {
    pageNo = lastPage;
  }

  var params =
    "cmd=" +
    cmd +
    "&pageNo=" +
    pageNo +
    "&hiddenTotalCount=" +
    oidSayisi +
    "&lastPage=" +
    lastPage +
    "&";

  //baslangic ve bitis dizinlerini belirle
  var baslangicIndeksi = (pageNo - 1) * birSayfadaGosterilecekSay;
  var bitisIndeksi = 0;
  if (pageNo * birSayfadaGosterilecekSay > oidSayisi) bitisIndeksi = oidSayisi;
  else bitisIndeksi = pageNo * birSayfadaGosterilecekSay;

  //oidHazir parametresi cekilecek oid'lerin hazir oldugunu gosterecek
  params +=
    "oidHazir=" +
    true +
    "&baslangicIndeksi=" +
    baslangicIndeksi +
    "&bitisIndeksi=" +
    bitisIndeksi;

  //oid'leri parametrelere koy
  for (var i = baslangicIndeksi; i < bitisIndeksi; i++) {
    params += "&oid" + i + "=" + oidsForWindow[i];
  }

  var contentIdForPaging = windowID + "_content";
  loadAJAXUsingPOST("dispatch", contentIdForPaging, params);
}
function paketListesiGuncelle(paketOid, pad, durum, yuklemeZamani) {
  if (durum == "3") {
    if (document.getElementById("adTD_" + paketOid)) {
      document.getElementById("adTD_" + paketOid).innerHTML =
        "<a href=\"javascript:paketIcerigiGoster('" +
        paketOid +
        "', '" +
        pad +
        "','" +
        yuklemeZamani +
        "', false);\">" +
        pad +
        "</a>";
    }
    if (document.getElementById("durumTD_" + paketOid)) {
      document.getElementById("durumTD_" + paketOid).innerHTML =
        '<img src="images/ok.gif"/>&nbsp;' +
        labels["PAKETDURUM_KONTROL_EDILDI"];
    }
    if (document.getElementById("kontrolTD_" + paketOid)) {
      document.getElementById("kontrolTD_" + paketOid).innerHTML = "";
    }
    if (document.getElementById("radioTD" + paketOid)) {
      document.getElementById("radioTD" + paketOid).innerHTML = "";
    }
  } else if (durum == "0") {
    if (document.getElementById("durumTD_" + paketOid)) {
      document.getElementById("durumTD_" + paketOid).innerHTML =
        '<img src="images/err.gif"/>&nbsp;' + labels["PAKETDURUM_HATALI"];
    }
    if (document.getElementById("kontrolTD_" + paketOid)) {
      document.getElementById("kontrolTD_" + paketOid).innerHTML = "";
    }
    if (document.getElementById("radioTD" + paketOid)) {
      document.getElementById("radioTD" + paketOid).innerHTML = "";
    }
  }
}

function paketKontrol(frm) {
  var lst = selectedPaket(frm);
  if (lst.length == 0) {
    alert(messages["PAKETKONTROL_PAKETSECINIZ"]);
    return false;
  } else {
    var poid = lst[0];
    istekGonder(
      "dispatch",
      "pleaseWaitDivForCommon",
      "cmd=PAKETKONTROL&p=" + poid,
      labels["PAKET_KONTROL_SONUCU"],
      "paketWindow",
      createModal
    );
  }
}

function yuklemeTarihAraligiGecerli(basTar, bitTar) {
  var gunFarki = tarihGunFarkiBul(basTar, bitTar);
  if (gunFarki < 0) {
    alert(
      "Yükleme Başlangıç Tarihi, Bitiş Tarihi'nden büyük olamaz. Lütfen bilgilerinizi kontrol ediniz."
    );
    return false;
  }

  var baslangisTarihiYil = basTar.substring(0, 4);
  var bitisTarihiYil = bitTar.substring(0, 4);

  if (!(baslangisTarihiYil == bitisTarihiYil)) {
    alert(
      "Yükleme Tarih Aralığı'ndaki Başlangıç ve Bitiş Yılı aynı olmalıdır. Lütfen bilgilerinizi kontrol ediniz."
    );
    return false;
  }

  return true;
}

function tarihGunFarkiBul(baslangic, bitis) {
  var baslangicTarihi = new Date(
    baslangic.substring(0, 4),
    baslangic.substring(4, 6) - 1,
    baslangic.substring(6, 8)
  );
  var bitisTarihi = new Date(
    bitis.substring(0, 4),
    bitis.substring(4, 6) - 1,
    bitis.substring(6, 8)
  );
  var one_day = 1000 * 60 * 60 * 24;
  return Math.floor(
    (bitisTarihi.getTime() - baslangicTarihi.getTime()) / one_day
  );
}

function taxReturnSearchFormControl(formID) {
  var form = document.getElementById(formID);
  if (!form.sorguTipiZ.checked) {
    alert("Yükleme Tarih Aralığı seçilmelidir.");
    form.baslangicTarihiGun.focus();
    return false;
  } else {
    var basTar = form.baslangicTarihi.value;
    var bitTar = form.bitisTarihi.value;
    if (!yuklemeTarihAraligiGecerli(basTar, bitTar)) {
      form.baslangicTarihiGun.focus();
      return false;
    }
  }

  if (form.sorguTipiN.checked && form.sorguTipiT.checked) {
    alert(messages["BEYANNAMESORGU_TC_VKN_BIRLIKTE"]);
    form.vergiNo.focus();
    return false;
  }
  if (form.sorguTipiN.checked) {
    var vergiNo = form.vergiNo;
    if (vergiNo.value == "") {
      alert(messages["BEYANNAMESORGU_VKN_BOS"]);
      vergiNo.focus();
      return false;
    } else if (!isNumeric(vergiNo.value.trim())) {
      alert(messages["VK_NO_RAKAM_OLMALIDIR"]);
      vergiNo.focus();
      return false;
    } else if (vergiNo.value.trim().length != 10) {
      alert(messages["BEYANNAMESORGU_VKN_EKSIK"]);
      vergiNo.focus();
      return false;
    }
  }
  if (form.sorguTipiT.checked) {
    var tcKimlikNo = form.tcKimlikNo;
    if (tcKimlikNo.value == "") {
      alert(messages["BEYANNAMESORGU_TCKN_BOS"]);
      tcKimlikNo.focus();
      return false;
    } else if (!isNumeric(tcKimlikNo.value.trim())) {
      alert(messages["TCK_NO_RAKAM_OLMALIDIR"]);
      tcKimlikNo.focus();
      return false;
    } else if (tcKimlikNo.value.trim().length != 11) {
      alert(messages["BEYANNAMESORGU_TCKN_EKSIK"]);
      tcKimlikNo.focus();
      return false;
    }
  }
  var isSorguTipleriCheckedWithoutDurum =
    form.sorguTipiN.checked ||
    form.sorguTipiT.checked ||
    form.sorguTipiP.checked ||
    form.sorguTipiV.checked ||
    form.sorguTipiZ.checked ||
    form.sorguTipiB.checked;

  if (!(form.sorguTipiD.checked || isSorguTipleriCheckedWithoutDurum)) {
    alert(messages["BEYANNAMESORGU_ENAZ_BIR_KRITER"]);
    form.sorguTipiN.focus();
    return false;
  }
  if (form.sorguTipiD.checked && !isSorguTipleriCheckedWithoutDurum) {
    alert(messages["BEYANNAMESORGU_SADECE_DURUM"]);
    form.sorguTipiN.focus();
    return false;
  }
  return true;
}
function taxReturnSearchFormPost() {
  if (taxReturnSearchFormControl("taxReturnSearchForm")) {
    istekGonder(
      "dispatch",
      "pleaseWaitDivForCommon",
      getTaxReturnSearchFormParameters("taxReturnSearchForm"),
      labels["BEYANNAMELISTESI_BEYANNAMELISTESI"],
      "",
      createWindowForBynList
    );
  }
  return false;
}
function getTaxReturnSearchFormParameters(formID) {
  var paramsString = "";
  var form = document.getElementById(formID);
  paramsString = "cmd=" + form.cmd.value;
  if (form.sorguTipiN.checked) {
    paramsString += "&sorguTipiN=1&vergiNo=" + form.vergiNo.value;
  }
  if (form.sorguTipiT.checked) {
    paramsString += "&sorguTipiT=1&tcKimlikNo=" + form.tcKimlikNo.value;
  }
  if (form.sorguTipiB.checked) {
    paramsString += "&sorguTipiB=1&beyannameTanim=" + form.beyannameTanim.value;
  }
  if (form.sorguTipiP.checked) {
    paramsString += "&sorguTipiP=1&donemBasYil=" + form.donemBasYil.value;
    paramsString += "&donemBasAy=" + form.donemBasAy.value;
    paramsString += "&donemBitYil=" + form.donemBitYil.value;
    paramsString += "&donemBitAy=" + form.donemBitAy.value;
  }
  if (form.sorguTipiV.checked) {
    paramsString += "&sorguTipiV=1&vdKodu=" + form.vdKodu.value;
  }
  if (form.sorguTipiZ.checked) {
    paramsString += "&sorguTipiZ=1&bitisTarihi=" + form.bitisTarihi.value;
    paramsString += "&baslangicTarihi=" + form.baslangicTarihi.value;
  }
  if (form.sorguTipiD.checked) {
    paramsString += "&sorguTipiD=1&durum=" + getRadioCheckedValue(form.durum);
  }
  return paramsString;
}
function taxReturnSearchEskiMuhasebeciFormControl(formID) {
  var form = document.getElementById(formID);

  var vergiNo = form.vergiNo;
  if (vergiNo.value == "") {
    alert(messages["BEYANNAMESORGU_VKN_BOS_OLAMAZ"]);
    vergiNo.focus();
    return false;
  } else if (!isNumeric(vergiNo.value.trim())) {
    alert(messages["VK_NO_RAKAM_OLMALIDIR"]);
    vergiNo.focus();
    return false;
  } else if (vergiNo.value.trim().length != 10) {
    alert(messages["BEYANNAMESORGU_VKN_EKSIK"]);
    vergiNo.focus();
    return false;
  }
  return true;
}
