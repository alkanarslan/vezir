function makeUnderLine(obj) {
  obj.style.textDecoration = "underline";
  obj.style.cursor = "pointer";
}
function deleteUnderLine(obj) {
  obj.style.textDecoration = "";
}
var beyannameAraFormuHTMLKodu = null;
var paketAraFormuHTMLKodu = null;
var ozelIslemlerFormuHTMLKodu = null;
var odemePlaniAraFormuHTMLKodu = null;
var gorusOneriFormuHTMLKodu = null;
var oncekiYilGeciciVergiMiktarlariHTMLKodu = null;
var oncekiYilKGeciciVergiMiktarlariHTMLKodu = null;

function showWindowCreateIfNotExist(wId, wTitle) {
  var win = Windows.getWindow(wId);
  if (win && win.visible) {
    //pencere daha once acildiysa
    win.setTitle(wTitle);
    win.toFront();
  } else {
    win = new Window({
      id: wId,
      className: class_name,
      title: wTitle,
      width: 825,
      height: 350,
      top: WINDOW_TOP_OFFSET,
      left: 180,
      destroyOnClose: true,
      recenterAuto: false,
    });
    win.showCenter();
  }
  return win;
}
var bynAraTitle = labels["MENU_BEYANNAMEARA"];
function beyannameAraFormu() {
  if (beyannameAraFormuHTMLKodu != null) {
    var win = showWindowCreateIfNotExist("mainWindow", bynAraTitle);
    var cntntDiv = document.getElementById(win.getId() + "_content");
    cntntDiv.innerHTML = beyannameAraFormuHTMLKodu;
  } else
    istekGonder(
      "dispatch",
      "pleaseWaitDivForCommon",
      "cmd=BEYANNAMESORGU",
      bynAraTitle,
      "mainWindow",
      commonWindowProcess
    );
}

var paketAraTitle = labels["MENU_PAKETARA"];
function paketAraFormu() {
  if (paketAraFormuHTMLKodu != null) {
    var win = showWindowCreateIfNotExist("mainWindow", paketAraTitle);
    var cntntDiv = document.getElementById(win.getId() + "_content");
    cntntDiv.innerHTML = paketAraFormuHTMLKodu;
  } else
    istekGonder(
      "dispatch",
      "pleaseWaitDivForCommon",
      "cmd=PAKETSORGU",
      paketAraTitle,
      "mainWindow",
      commonWindowProcess
    );
}

