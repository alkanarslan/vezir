function beyannameGoruntule(oid, arsivdenGoruntule, asAttachment) {
  callMenuUrlPopUp(
    "dispatch?cmd=IMAJ&subcmd=BEYANNAMEGORUNTULE&TOKEN=" +
      getTOKEN() +
      "&beyannameOid=" +
      oid +
      getParameterForArsiv(arsivdenGoruntule) +
      (asAttachment ? "" : "&inline=true"),
    "goruntuleme"
  );
}
