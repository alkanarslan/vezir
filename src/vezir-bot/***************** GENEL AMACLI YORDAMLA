function beyannameTopluOnay() {
  WINDOW_TITLE = labels["ONAYSONUC_BASLIK"];
  WINDOW_ID = "onayIptalSonuclariWindow";

  var divId = createModal();
  var div = document.getElementById(divId);
  div.innerHTML =
    '<table id="sonucListesi" cellspacing=1 cellpadding=2 bgcolor=gray></table>';
  siradakiBeyIsle("cmd=BEYANNAMEONAY&o=");
}
function beyannameTopluIptal() {
