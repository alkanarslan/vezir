/***************** GENEL AMACLI YORDAMLAR *********************/
function formatStr(msg) {
  var A = Array.prototype.slice.call(arguments, 1);
  var spl = msg.split("@@");
  var ret = "";

  for (var i = 0; i < spl.length - 1; i++) ret += spl[i] + A[i];
  return ret + spl[i];
}
function pressedKey(e) {
  var keynum;
  var keychar;
  var numcheck;
  if (window.event) {
    // IE
    keynum = e.keyCode;
  } else if (e.which) {
    // Netscape/Firefox/Opera
    keynum = e.which;
  }
  keychar = String.fromCharCode(keynum);
  if (keynum == 27) {
    //27=ESC
    Windows.closeAllModalWindows();
  }
}
function pleaseWait(divName, msg) {
  if (!msg) msg = labels["LUTFEN_BEKLEYINIZ"];
  var d = document.getElementById(divName);
  if (d) {
    d.style.display = "";
    d.innerHTML =
      '<center><img style="vertical-align: middle;" src="images/progress.gif"/>&nbsp;<span style="vertical-align:middle">' +
      msg +
      "</span></center>";
  }
}
function clearPleaseWait(divName) {
  var d = document.getElementById(divName);
  if (d) {
    d.innerHTML = "";
    d.style.display = "none";
  }
}
function setElementContent(divName, value) {
  var d = document.getElementById(divName);
  if (d) d.innerHTML = value;
}
function clearDiv(divName) {
  var d = document.getElementById(divName);
  if (d) d.innerHTML = "";
}
//writeIframeContent, belirtilen id'li iframe icine parametre gecilen icerigi yazar.
function writeIframeContent(iframeID, content) {
  var testFrame = document.getElementById(iframeID);
  var doc = testFrame.contentDocument;
  if (doc == undefined || doc == null)
    //farkli tarayici uyumlulugu icin eklendi
    doc = testFrame.contentWindow.document;
  doc.open();
  doc.write(content);
  doc.close();
}

/***************** SAYFALARA OZGU YORDAMLAR *********************/
var secilenBynOidListesi = [];
var islemGorecekByn = 0;

function muhsgkVarMi(frm) {
  var rows = frm.getElementsByTagName("tr");
  var i = 0;
  var row;
  debugger;
  while ((row = rows[i++])) {
    if (row.cells.length > 2) {
      if (row.cells[0].getElementsByTagName("input").length == 0) continue;
      var cbox = row.cells[0].getElementsByTagName("input")[0];
      var bturu = row.cells[1];

      if (cbox.type == "checkbox" && cbox.id != "hepsi") {
        if (cbox.checked && bturu.innerHTML == "MUHSGK") {
          return true;
        }
      }
    }
  }
  return false;
}

function yrti7186VarMi(frm) {
  var rows = frm.getElementsByTagName("tr");
  var i = 0;
  var row;
  debugger;
  while ((row = rows[i++])) {
    if (row.cells.length > 2) {
      if (row.cells[0].getElementsByTagName("input").length == 0) continue;
      var cbox = row.cells[0].getElementsByTagName("input")[0];
      var bturu = row.cells[1];

      if (cbox.type == "checkbox" && cbox.id != "hepsi") {
        if (cbox.checked && bturu.innerHTML == "YRTI7186") {
          return true;
        }
      }
    }
  }
  return false;
}

function selectedByn(frm) {
  var ret = [];
  var inp;
  var i = 0;
  var inputs = frm.getElementsByTagName("input");
  while ((inp = inputs[i++])) {
    if (inp.type == "checkbox" && inp.id != "hepsi") {
      if (inp.checked) {
        ret[ret.length] = inp.name;
      }
    }
  }
  return ret;
}

function beyannameIslemButonlariniDuzenle(frm) {
  var lst = selectedByn(frm);

  if (lst.length == 0 || lst.length > 1) {
    if (
      document.getElementById("gvk281BildirimindenMuhBeyOlusturButtonId") !=
      null
    )
      document.getElementById(
        "gvk281BildirimindenMuhBeyOlusturButtonId"
      ).style.display = "none"; //

    if (
      document.getElementById("gvksgkBildirimindenMuhBeyOlusturButtonId") !=
      null
    )
      document.getElementById(
        "gvksgkBildirimindenMuhBeyOlusturButtonId"
      ).style.display = "none"; //
  }
  if (lst.length == 1 && beyannameTipleri[lst[0]] == "MUHGVK281") {
    document.getElementById(
      "gvk281BildirimindenMuhBeyOlusturButtonId"
    ).style.display = ""; //
  }
  if (lst.length == 1 && beyannameTipleri[lst[0]] == "MUHGVKSGK") {
    document.getElementById(
      "gvksgkBildirimindenMuhBeyOlusturButtonId"
    ).style.display = ""; //
  }
}

function selectedPaket(frm) {
  var ret = [];
  var inp;
  var i = 0;
  var inputs = frm.getElementsByTagName("input");
  while ((inp = inputs[i++])) {
    if (inp.type == "radio") {
      if (inp.checked) {
        ret[ret.length] = inp.id;
      }
    }
  }
  return ret;
}

function selectAll(cb) {
  var checked = true;
  var i = 0;
  if (!cb.checked) {
    checked = false;
  }
  var inputs = cb.form.getElementsByTagName("input");
  var inp = inputs[i];
  do {
    if (inp.type == "checkbox" && inp.id != "hepsi") {
      if (inp != null) {
        inputs[i].checked = checked;
      }
    }
    i++;
  } while ((inp = inputs[i]));
}

function onaySec(frm) {
  var lst = selectedByn(frm);
  if (lst.length == 0) {
    alert(messages["BEYANNAMEONAY_ENAZ_BIR_BEYANNAME"]);
    return;
  }
  islemGorecekByn = 0;
  secilenBynOidListesi = lst;
  if (lst.length == 1) {
    if (muhsgkVarMi(frm)) {
      beyannameSGKBildirimleriGoster(lst[0]);
      return;
    }

    if (yrti7186VarMi(frm)) {
      if (
        !confirm(messages["BEYANNAMEONAY_YRTI7186_ONAYLAMAK_ISTIYOR_MUSUN"])
      ) {
        return;
      }
    }

    if (
      confirm(
        messages["BEYANNAMELISTESI_ONAYLAMAK_ISTIYOR_MUSUN"]
      ) /*&& confirm(messages['BEYANNAMEONAY_OZELONAY_6111ODEMEUYARISI'])*/
    )
      beyannameTopluOnay();
  } else {
    /*if(muhsgkVarMi(frm)){
            alert("Toplu onayladığınız beyannameler içerisinde MUHSGK beyannamesi olduğundan dolayı toplu onay yapamazsınız.")
            return ;
        }*/

    if (yrti7186VarMi(frm)) {
      if (
        !confirm(messages["BEYANNAMEONAY_YRTI7186_ONAYLAMAK_ISTIYOR_MUSUN2"])
      ) {
        return;
      }
    }

    if (
      confirm(
        messages["BEYANNAMELISTESI_BIRDENFAZLABYN_ONAYLAMAK_ISTIYOR_MUSUN"]
      ) /*&& confirm(messages['BEYANNAMEONAY_OZELONAY_6111ODEMEUYARISI'])*/
    ) {
      beyannameTopluOnay();
    }
  }
}

function ozelOnaySec(frm) {
  var lst = selectedByn(frm);
  if (lst.length == 0) {
    alert(messages["BEYANNAMEONAY_BIR_BEYANNAME_SECIN"]);
    return;
  }
  if (lst.length != 1) {
    alert(messages["BEYANNAMEOZELONAY_SADECE_BIR_BEYANNAME"]);
    return;
  }
  islemGorecekByn = 0;
  secilenBynOidListesi = [];
  if (
    confirm(
      messages["BEYANNAMELISTESI_OZELONAYLAMAK_ISTIYOR_MUSUN"]
    ) /*&& confirm(messages['BEYANNAMEONAY_OZELONAY_6111ODEMEUYARISI'])*/
  )
    beyannameOzelOnayInDetay(lst[0]);
}

function iptalSec(frm) {
  var lst = selectedByn(frm);
  if (lst.length == 0) {
    alert(messages["BEYANNAMEONAY_ENAZ_BIR_BEYANNAME"]);
    return;
  }
  islemGorecekByn = 0;
  secilenBynOidListesi = lst;
  if (lst.length == 1) {
    if (confirm(messages["BEYANNAMELISTESI_IPTAL_ETMEK_ISTIYOR_MUSUN"]))
      beyannameTopluIptal();
  } else {
    if (
      confirm(
        messages["BEYANNAMELISTESI_BIRDENFAZLABYN_IPTAL_ETMEK_ISTIYOR_MUSUN"]
      )
    ) {
      beyannameTopluIptal();
    }
  }
}

function iptalSecEskiMuhasebeci(frm) {
  var lst = selectedByn(frm);
  if (lst.length == 0) {
    alert(messages["BEYANNAMEONAY_ENAZ_BIR_BEYANNAME"]);
    return;
  }
  islemGorecekByn = 0;
  secilenBynOidListesi = lst;
  if (lst.length == 1) {
    if (confirm(messages["BEYANNAMELISTESI_IPTAL_ETMEK_ISTIYOR_MUSUN"]))
      beyannameEskiMuhasebeciTopluIptal();
  } else {
    if (
      confirm(
        messages["BEYANNAMELISTESI_BIRDENFAZLABYN_IPTAL_ETMEK_ISTIYOR_MUSUN"]
      )
    ) {
      beyannameEskiMuhasebeciTopluIptal();
    }
  }
}

function sonucListesineSatirEkle(tbl, idx) {
  // <tr bgcolor="white"><td width="20px" align=right>' + (idx+1) + '</td><td id="' + nextDiv + '"></td></tr>;
  var row = tbl.insertRow(idx);
  row.bgColor = "white";

  // first cell will include row nr
  var nrCell = row.insertCell(0);
  var textNode = document.createTextNode(idx + 1);
  nrCell.width = "20px";
  nrCell.align = "right";
  nrCell.appendChild(textNode);

  // second cell will include message
  var msgCell = row.insertCell(1);
  msgCell.width = "798px";
  msgCell.id = "sonuc" + idx;
}

function siradakiBeyIsle(params) {
  var idx = islemGorecekByn++;
  var oid = secilenBynOidListesi[idx];

  var sld = document.getElementById("sonucListesi");
  var nextDiv = "sonuc" + idx;
  //sld.innerHTML += '<tr bgcolor="white"><td width="20px" align=right>' + (idx+1) + '</td><td id="' + nextDiv + '"></td></tr>';
  sonucListesineSatirEkle(sld, idx);

  loadAJAXUsingPOST("dispatch", nextDiv, params + oid);
}
